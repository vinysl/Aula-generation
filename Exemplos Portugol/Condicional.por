programa
{
	
	funcao inicio()
	{
		inteiro idade
		escreva("Entre com a sua idade: ")
		leia(idade)
		se (idade>=18)
		{
			escreva("Você é maior de idade")
		}
		senao
	{
		escreva("Você não é maior de idade")
	}

	}
}
